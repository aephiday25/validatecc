validity <- function(ccnumber){
ifelse((nchar(ccnumber) == 16 | nchar(ccnumber) == 14) & tbl_genap(ccnumber) %% 10 == 0 & cardtype(ccnumber) %in% names(jenis_kartu), "Yes",
ifelse((nchar(ccnumber) == 13 | nchar(ccnumber) == 15) & tbl_ganjil(ccnumber) %% 10 == 0 & cardtype(ccnumber) %in% names(jenis_kartu), "Yes", "Nope"))
}
